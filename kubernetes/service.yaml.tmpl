apiVersion: v1
kind: Service
metadata:
  namespace: {{.NodeName}}
  labels:
    app: {{.NodeName}}
  name: {{.NodeName}}
spec:
  ports:
  {{if .IsRestServer }}
    - protocol: TCP
      port: {{.RestServerPort}}
      targetPort: {{.RestServerPort}}
   {{end}}
  selector:
    app: {{.NodeName}}
  type: ClusterIP
